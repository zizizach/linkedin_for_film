<ul>
    <li><%= link_to 'Home', root_path %></li>
</ul>
<ul>
    <% if user_signed_in? %>
    <li><%= link_to 'Edit Profile', current_user.profile.present? ? edit_profile_path(current_user.profile) : new_profile_path %></li>
    <li><%= link_to 'Sign Out', destroy_user_session_path, method: :delete %></li>
    <% else %>
    <li><%= link_to 'Sign In', new_user_session_path %></li>
    <% end %>
</ul>